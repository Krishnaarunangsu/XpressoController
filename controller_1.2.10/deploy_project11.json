{
	"name": "pipeline_project25",
	"target_environment_type": "kubernetes",
	"target_environment": "DEV",
	"components": {
		"service_reader": {
			"replicas": 2,
			"build_version": 4,
			"environment": [],
			"ports": [{
				"name": "default",
				"port": 5000
			}, {
				"name": "app1_port_new",
				"port": 5051
			}],
			"persistence": {
				"mount_path": "/app/data/"
			},
			"is_external": true
		}

	},
	"pipelines": {
		"fetch_data": {
			"components": {
				"service_reader": {
					"mount_path": "/app/data",
					"custom_docker_image": "dockerregistry.xpresso.ai/xprops/pipeline_project25/service_reader--master:4"
				}
			}
		}
	}

}

